#include <iostream>
#include <string>
using namespace std;
int main()
{
	string s = "jksdhfksjhdkfjkskdsldk;llsldk;lsdk;sldla'ad/'''''ddda;;;;;;;;.d,faadnkahdlkakjdbmadbckjabdkjabdj";
	char a;
	int b=1,c=0;
	for (int i = 0; i < s.length(); i++) {
		b = 1;
		for (int j = 0;j < s.length();j++) {
			if (i != j) {
				if (s[i] == s[j]) 
					b++;				
			}
		}
		if (b > c) {
			a = s[i];
			c = b;
		}
	}
	cout << "simvol -- '" << a<<"'"<< endl << "kolichestvo raz -- " << c;
   
}
